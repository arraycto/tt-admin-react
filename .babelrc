{
    "presets": [
        "env",
        "stage-0",
        "react"
    ],
    "plugins": [
        "transform-decorators-legacy",
        "react-activation/babel",
        "transform-runtime",
        [
            "import",
            {
                "libraryName": "antd",
                "libraryDirectory": "es",
                "style": "css" // `style: true` 会加载 less 文件
            }
        ]
    ]
}